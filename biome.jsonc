{
  "$schema": "https://biomejs.dev/schemas/2.2.0/schema.json",
  "extends": ["ultracite"],
  "linter": {
    "rules": {
      "style": {
        "noMagicNumbers": "off"
      },
      "suspicious": {
        "noConsole": "off", // will allow to use console.log
        "noExplicitAny": "off" // will allow to use type of <any>
      }
    }
  }
}
